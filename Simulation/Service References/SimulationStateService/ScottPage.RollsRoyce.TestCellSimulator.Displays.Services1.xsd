      <xs:element minOccurs="0" name="TurbineGasTemperature" nillable="true" type="tns:ParameterValue" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="SimulationParameters" nillable="true" type="tns:SimulationParameters" />
